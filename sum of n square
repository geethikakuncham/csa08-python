def sum_of_squares(n):
    total_sum = 0
    for i in range(1, n + 1):
        total_sum += i**2
    return total_sum
def main():
    n = int(input("Enter the value of N: "))
    result = sum_of_squares(n)
    print("The sum of squares from 1 to", n, "is:", result)
if __name__ == "__main__":
    main()
